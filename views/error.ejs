<% include ./layout/top %>

<h2>Error: <%= err.status %></h2>
<pre><%= err.stack %></pre>

<div style="text-align: center;">
	<% if(err.status == 404){ %>
		<img src="/images/error_404.jpg" alt="" style="margin:15px 15px 15px 15px">
	<% } %>

	<% if(err.status == 403){ %>
		<img src="/images/error_403.jpg" alt="" style="margin:15px 15px 15px 15px">
	<% } %>

</div>

<% include ./layout/bottom %>